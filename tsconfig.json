{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": "./",
        "paths": {
            "@assets/*": ["assets/*"],
            "@components/*": ["components/*"],
            "@contexts/*": ["contexts/*"],
            "@db/*": ["db/*"],
            "@hooks/*": ["hooks/*"],
            "@services/*": ["services/*"],
            "@types/*": ["types/*"],
            "@utils/*": ["utils/*"]
        }
    },
    "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
